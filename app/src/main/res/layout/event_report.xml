        <LinearLayout
            android:id="@+id/main_lll"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginLeft="0dp"
            android:layout_marginRight="0dp"
            android:orientation="horizontal"
            android:weightSum="2"

            >

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:background="@drawable/full_rounded_white"
                >

                <LinearLayout
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_marginLeft="1dp"
                    android:layout_gravity="center_vertical"
                    android:background="@drawable/circle">
                    <ImageView
                        android:id="@+id/vehicleListIcn"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/truckicon2"
                        android:padding="5dp"/>
                </LinearLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:paddingBottom="4dp"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:paddingLeft="1dp"
                    android:paddingRight="6dp"
                    android:paddingTop="6dp">

                    <TextView
                        android:id="@+id/eventDevice_name"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:layout_marginLeft="4dp"
                        android:text=""
                        android:textColor="@color/colorPrimary"
                        android:textSize="13dp" />

                </RelativeLayout>
            </LinearLayout>

            <View
                android:layout_width="3dp"
                android:layout_height="match_parent"
                android:background="#EFF5F1"/>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:background="@drawable/full_rounded_white">

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:background="#ffffff"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/upadated"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="type"
                        android:textAllCaps="true"
                        android:textColor="@color/app_body_text_2"
                        android:textSize="12dp"
                        />
                    <TextView
                        android:id="@+id/eventType_tv"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/upadated"
                        android:gravity="center"
                        android:background="#ffffff"
                        android:layout_marginTop="2dp"
                        android:textAllCaps="true"
                        android:text="type"
                        android:textColor="@color/colorPrimary"
                        android:textSize="10dp"
                        />
                </RelativeLayout>
            </LinearLayout>
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_height="2dp"
            android:background="#E0E0E0"/>

        <LinearLayout
            android:id="@+id/main_ll"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginLeft="0dp"
            android:layout_marginRight="0dp"
            android:orientation="horizontal"
            android:weightSum="2"

            >

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:background="@drawable/full_rounded_white"
                >

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:paddingBottom="4dp"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:paddingLeft="1dp"
                    android:paddingRight="6dp"
                    android:paddingTop="6dp">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:background="#ffffff"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/upadatede"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:text="time"
                            android:textAllCaps="true"
                            android:textColor="@color/app_body_text_2"
                            android:textSize="12dp"
                            />
                        <TextView
                            android:id="@+id/eventTime_tv"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="#ffffff"
                            android:gravity="center"
                            android:text="hello"
                            android:textColor="@color/colorPrimary"
                            android:textSize="10dp"
                            android:layout_below="@+id/upadatede"


                            />
                    </RelativeLayout>

                </RelativeLayout>
            </LinearLayout>

            <View
                android:layout_width="3dp"
                android:layout_height="match_parent"
                android:background="#EFF5F1"/>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:background="@drawable/full_rounded_white">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:paddingBottom="4dp"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:paddingLeft="1dp"
                    android:paddingRight="6dp"
                    android:paddingTop="6dp">

                    <TextView
                        android:id="@+id/upadatedee"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="geofence"
                        android:textAllCaps="true"
                        android:textColor="@color/app_body_text_2"
                        android:textSize="12dp"
                        />
                    <TextView
                        android:id="@+id/eventGeofence_tv"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/upadatedee"
                         android:gravity="center"
                        android:background="#ffffff"
                        android:text="geofence"
                        android:textColor="@color/colorPrimary"
                        android:textSize="10dp"
                        />
                </RelativeLayout>
            </LinearLayout>
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_height="2dp"
            android:background="#E0E0E0"/>

    </LinearLayout>
</android.support.v7.widget.CardView>